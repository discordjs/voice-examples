{
	"$schema": "https://turborepo.org/schema.json",
	"tasks": {
		"build:check": {
			"dependsOn": ["^build:check"],
			"outputs": []
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"lint": {
			"dependsOn": ["build"],
			"outputs": []
		},
		"format": {
			"outputs": []
		},
		"fmt": {
			"outputs": []
		}
	}
}
